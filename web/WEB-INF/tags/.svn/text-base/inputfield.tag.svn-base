<%@tag description="Input Field Tag" pageEncoding="UTF-8"%>
<%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%> 
<%@attribute name="label" required="true"%>
<%--
 Within this tag, the value of the field is available to the body as ${row[field]} where field
 is the name of the database column.
--%>
<tr class="textfieldrow">
<td class="textfieldlabel">${label}:</td>
<td class="textfieldvalue">
  <jsp:doBody/>
</td>
</tr>

